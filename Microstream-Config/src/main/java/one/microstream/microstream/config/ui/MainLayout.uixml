<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.vaadin.flow.component.button.Button?>
<?import com.vaadin.flow.component.combobox.MultiSelectComboBox?>
<?import com.vaadin.flow.component.grid.Grid?>
<?import com.vaadin.flow.component.html.Div?>
<?import com.vaadin.flow.component.html.Hr?>
<?import com.vaadin.flow.component.orderedlayout.HorizontalLayout?>
<?import com.vaadin.flow.component.orderedlayout.VerticalLayout?>
<?import com.vaadin.flow.component.radiobutton.RadioButtonGroup?>
<?import com.vaadin.flow.component.textfield.NumberField?>
<?import java.lang.String?>
<?import one.microstream.microstream.config.domain.Action?>
<VerticalLayout xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="MainLayout" x:route="/">
    <x:constraints zpos="0" width="100%" height="100%" />
    <Div x:name="div">
        <x:constraints width="90%" alignment="CENTER" height="80%" flex-grow="0.0" />
        <HorizontalLayout x:name="horizontalLayout">
            <x:constraints width="100%" alignment="AUTO" height="100%" flex-grow="0.0" />
            <VerticalLayout spacing="false" x:name="verticalLayout" padding="false">
                <x:constraints width="300px" height="100%" flex-grow="0.0" />
                <Button x:themeVariants="LUMO_SUCCESS" x:name="btnStart" text="Start">
                    <x:icon src="com.vaadin.flow.component.icon.VaadinIcon#PLAY" />
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                    <x:event>click.onComponentEvent</x:event>
                </Button>
                <Button enabled="false" x:themeVariants="LUMO_ERROR" x:name="btnStop" text="Stop">
                    <x:icon src="com.vaadin.flow.component.icon.VaadinIcon#STOP" />
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                    <x:event>click.onComponentEvent</x:event>
                </Button>
                <Hr x:name="hr">
                    <x:constraints width="0px" height="0px" flex-grow="0.0" />
                </Hr>
                <RadioButtonGroup x:name="radioButtonGroup" x:vertical="true">
                    <x:typeArguments>
                        <typeArgument type="String" />
                    </x:typeArguments>
                    <x:constraints width="0px" height="0px" flex-grow="0.0" />
                </RadioButtonGroup>
                <MultiSelectComboBox x:name="cmbTargets" autoExpand="com.vaadin.flow.component.combobox.MultiSelectComboBox.AutoExpandMode.BOTH" label="Task Targets">
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                    <x:typeArguments>
                        <typeArgument type="String" />
                    </x:typeArguments>
                    <x:event>valueChange.valueChanged</x:event>
                </MultiSelectComboBox>
                <Hr x:name="hr2">
                    <x:constraints width="0px" height="0px" flex-grow="0.0" />
                </Hr>
                <NumberField x:name="rangeAmountThreads" value="1.0" label="Amount Threads">
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                </NumberField>
                <NumberField x:name="rangeRunCount" value="1.0" label="Run Count">
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                </NumberField>
                <NumberField x:name="rangeSendDelay" value="0.0" label="Send Delay (ms)">
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                </NumberField>
                <Hr x:name="hr3">
                    <x:constraints width="0px" height="0px" flex-grow="0.0" />
                </Hr>
                <NumberField x:name="rangeRandomSeed" value="123456.0" label="Random Seed">
                    <x:constraints width="100%" height="0px" flex-grow="0.0" />
                </NumberField>
            </VerticalLayout>
            <Grid x:themeVariants="LUMO_ROW_STRIPES" x:name="grid">
                <x:typeArguments>
                    <typeArgument type="Action" />
                </x:typeArguments>
                <x:constraints width="-1px" height="100%" flex-grow="0.0" />
                <x:columns>
                    <column property="date" header="Time" sortable="true" autoWidth="true" flexGrow="0">
                        <renderer nullRepresentation="" type="LocalDateTimeRenderer">
                            <dateTimeFormatter zone="default" format="LocalizedDateTime" timeFormatStyle="SHORT" dateFormatStyle="MEDIUM" locale="default" />
                        </renderer>
                    </column>
                    <column property="description" header="Action" sortable="true" autoWidth="true" />
                </x:columns>
            </Grid>
        </HorizontalLayout>
    </Div>
</VerticalLayout>
